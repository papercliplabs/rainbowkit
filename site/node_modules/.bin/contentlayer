#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/spencerperkins/Developer/Paperclip/rainbowkit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/spencerperkins/Developer/Paperclip/rainbowkit/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/contentlayer@0.2.2/node_modules/contentlayer/bin/cli.cjs" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/contentlayer@0.2.2/node_modules/contentlayer/bin/cli.cjs" "$@"
fi
